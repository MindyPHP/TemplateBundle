{
    "name": "mindy/template-bundle",
    "type": "symfony-bundle",
    "description": "Mindy TemplateBundle",
    "keywords": [
        "template",
        "templating"
    ],
    "homepage": "https://mindy-cms.com",
    "license": "MIT",
    "authors": [
        {
            "name": "Falaleev Maxim",
            "email": "max@mindy-cms.com"
        }
    ],
    "require": {
        "php": ">=5.5.9",
        "mindy/template": "~3.0",
        "mindy/finder": "~3.0",
        "symfony/dependency-injection": "~3.0",
        "symfony/config": "~3.0",
        "symfony/http-kernel": "~3.0",
        "symfony/expression-language": "~3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~5.0"
    },
    "autoload": {
        "psr-4": {
            "Mindy\\Bundle\\TemplateBundle\\": ""
        },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "minimum-stability": "dev",
    "extra": {
        "branch-alias": {
            "dev-master": "3.6.8-dev"
        }
    }
}
