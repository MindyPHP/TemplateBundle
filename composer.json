{
    "name": "mindy/template-bundle",
    "type": "symfony-bundle",
    "description": "Mindy TemplateBundle",
    "keywords": [
        "template",
        "templating"
    ],
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Falaleev Maxim",
            "email": "max@mindy-cms.com"
        }
    ],
    "require": {
        "php": ">=7.1",
        "mindy/template": "~4.0",
        "symfony/config": "~4.0",
        "symfony/dependency-injection": "~4.0",
        "symfony/expression-language": "~4.0",
        "symfony/yaml": "~4.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.5",
        "symfony/framework-bundle": "~4.0",
        "symfony/http-kernel": "~4.0",
        "symfony/security-bundle": "~4.0",
        "symfony/var-dumper": "~4.0"
    },
    "autoload": {
        "psr-4": {
            "Mindy\\Bundle\\TemplateBundle\\": ""
        },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Mindy\\Bundle\\TemplateBundle\\Tests\\": ""
        }
    }
}
